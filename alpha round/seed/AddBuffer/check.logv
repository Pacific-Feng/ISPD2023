[02/13 08:36:01     88s] @file 17: set p_names {VDD VSS}
[02/13 08:36:01     88s] @file 18:
[02/13 08:36:01     88s] @file 19: set c_flag "valid"
[02/13 08:36:01     88s] @file 20:
[02/13 08:36:01     88s] @file 21: # the loop strats from M2 layer and continue checking until M9
[02/13 08:36:01     88s] @file 22: for {set j 2} {$j < 10} {incr j} {
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s]  set coors  ""
[02/13 08:36:01     88s]  set sub_result ""
[02/13 08:36:01     88s]  set refrence ""
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s] # a loop for selecting VDD and VSS for each metal layer 
[02/13 08:36:01     88s]  for {set k 0} {$k < 2} {incr k} {
[02/13 08:36:01     88s] select_routes -nets [lindex $p_names $k] -shapes stripe -layer M$j
[02/13 08:36:01     88s] # the stripes on the even layers are horizontal while the rest are vertical
[02/13 08:36:01     88s]  if {[expr $j / 2] == 0} {
[02/13 08:36:01     88s] # if the stripe is horizontal, we capture the lly coordinates of the stripes
[02/13 08:36:01     88s]   set coors [get_db selected .rect.ll.y]
[02/13 08:36:01     88s]   } else {
[02/13 08:36:01     88s] # if the stripe is vertical, we capture the llx coordinates of the stripes
[02/13 08:36:01     88s]    set coors [get_db selected .rect.ll.x]
[02/13 08:36:01     88s]    }
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s]  set coors [lsort -dictionary $coors]
[02/13 08:36:01     88s]  for {set i 0} {$i < [expr [llength $coors] - 1]} {incr i} {
[02/13 08:36:01     88s]   set sub_result [expr [lindex $coors [ expr $i + 1]] - [lindex $coors $i] ]
[02/13 08:36:01     88s]      if {$i == 0} {
[02/13 08:36:01     88s]       set refrence [expr int($sub_result)]
[02/13 08:36:01     88s]      }
[02/13 08:36:01     88s] # here we compare the integer part for each coordinate pair to measure the distance
[02/13 08:36:01     88s]      if {[expr int($sub_result)] == $refrence} {
[02/13 08:36:01     88s]       set compare "valid"
[02/13 08:36:01     88s]      } else {
[02/13 08:36:01     88s]        set compare "false"
[02/13 08:36:01     88s]        set c_flag "false"
[02/13 08:36:01     88s] 	   puts " [expr int($sub_result)] != $refrence"
[02/13 08:36:01     88s]    	  }
[02/13 08:36:01     88s]  }
[02/13 08:36:01     88s]  deselect_obj -all
[02/13 08:36:01     88s] # detailed report 
[02/13 08:36:01     88s]  puts $out "M$j ---- [lindex $p_names $k]  ---> $compare ---- $c_flag"
[02/13 08:36:01     88s]  }
[02/13 08:36:01     88s] }
