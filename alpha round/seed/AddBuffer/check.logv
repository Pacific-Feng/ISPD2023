[02/13 08:36:01     88s] @file 15: # extract design geometries
[02/13 08:36:01     88s] @file 16: set core_rect [get_db designs .core_bbox]
[02/13 08:36:01     88s] @file 17: set x_l_core [lindex $core_rect 0 0]
[02/13 08:36:01     88s] @file 18: set y_l_core [lindex $core_rect 0 1]
[02/13 08:36:01     88s] @file 19: set x_unit [lindex [get_db sites .size] 0 0]
[02/13 08:36:01     88s] @file 20: set y_unit [lindex [get_db sites .size] 0 1]
[02/13 08:36:01     88s] @file 21:
[02/13 08:36:01     88s] @file 22: puts $out "CORE_BBOX [lindex [get_db designs .core_bbox] 0]"
