[02/13 08:36:01     88s] @file 24: puts $out "NUM_SITES_PER_ROW [get_db [lindex [get_db rows] 0] .num_x]"
[02/13 08:36:01     88s] @file 25: puts $out "SITE_WIDTH $x_unit "
[02/13 08:36:01     88s] @file 26: puts $out "SITE_HEIGHT $y_unit "
[02/13 08:36:01     88s] @file 27:
[02/13 08:36:01     88s] @file 28: # extract cell placement; note that physical cells (decaps, fillers, tapcells) are already deleted at this point
[02/13 08:36:01     88s] @file 29: foreach cell [get_db insts] {
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s] 	set cell_rect [lindex [get_db $cell .bbox] 0]
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s] 	set x_pos_core [expr [lindex $cell_rect 0] - $x_l_core]
[02/13 08:36:01     88s] 	set y_pos_core [expr [lindex $cell_rect 1] - $y_l_core]
[02/13 08:36:01     88s] 	set x_width [expr [lindex $cell_rect 2] - [lindex $cell_rect 0]]
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s] 	set x_idx [expr int(($x_pos_core + 0.001) / $x_unit)]
[02/13 08:36:01     88s] 	set y_idx [expr int(($y_pos_core + 0.001) / $y_unit)]
[02/13 08:36:01     88s] 	set width [expr int(($x_width + 0.001) / $x_unit)]
[02/13 08:36:01     88s] 
[02/13 08:36:01     88s] 	puts $out "[get_db $cell] $x_idx $y_idx $width"
[02/13 08:36:01     88s] }
